{
  "name": "Texin",
  "version": "0.1.0",
  "description": "Pin to rollback.",
  "manifest_version": 3,

  "permissions": ["storage","notifications"],

  "action": {
    "default_title": "Texin",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}

